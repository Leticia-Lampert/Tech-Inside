{
  "database": {
    "rules": "database.rules.json"
  },
    "hosting": {
      "appAssociation": "AUTO",
    "rewrites": [ {
      "source": "https://maxum-case.app/",
      "dynamicLinks": true,
      "destination": "/index.html"
    } ],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
